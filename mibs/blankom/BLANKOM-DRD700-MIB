					INTEGER,
				neGroup4AccessPage
					DisplayString,
				neGroup4AccessLevel
					INTEGER
			 }

		neGroupAccessIndex OBJECT-TYPE
			SYNTAX INTEGER (1..256)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Index Position."
			::= { neGroupAccessEntry 1 }

		neGroup1AccessPage OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..32))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Page."
			::= { neGroupAccessEntry 2 }

