				"NTP Sync Interval: 1-9999 min"
			::= { date_time 7 }

		neConfigDaylightSavingTime OBJECT-TYPE
			SYNTAX INTEGER (0..1)
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Daylight Saving Time: OFF(0), EUROPEAN(1)"
			::= { date_time 8 }

		neConfigSyncThreshold OBJECT-TYPE
			SYNTAX INTEGER (1..99)
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Sync Threshold: 1-99 sec"
			::= { date_time 9 }

 		neConfigFallbackTime OBJECT-TYPE
			SYNTAX INTEGER (0..10)
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Fallback Time: 10 minutes(0), 20 minutes(1), 30 minutes(2), 40 minutes(3),
				50 minutes(4), 60 minutes(5), 120 minutes(6), 240 minutes(7), 360 minutes(8),
				480 minutes(9), 600 minutes(10)"
			::= { user_accounts 1 }

		neUserAccountTable OBJECT-TYPE
			SYNTAX SEQUENCE OF NeUserAccountEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"Table containing information about each Users."
			::= { user_accounts 20 }

		neUserAccountEntry OBJECT-TYPE
			SYNTAX NeUserAccountEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"List of information about each Users."
			INDEX { neUserAccountIndex }
			::= { neUserAccountTable 1 }

		NeUserAccountEntry ::=
			SEQUENCE { 
				neUserAccountIndex
					INTEGER,
				neUserAccountName
					DisplayString,
				neUserAccountPassword
					DisplayString,
				neUserAccountRowStatus
					INTEGER
			 }

		neUserAccountIndex OBJECT-TYPE
			SYNTAX INTEGER (1..256)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Index Position."
			::= { neUserAccountEntry 1 }

		neUserAccountName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..32))
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Name."
			::= { neUserAccountEntry 2 }

		neUserAccountPassword OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..32))
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Password."
			::= { neUserAccountEntry 3 }

		neUserAccountRowStatus OBJECT-TYPE
			SYNTAX INTEGER (0..6)
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"OFF(0), ADMIN(1), READONLY(2), GROUP 1(3), GROUP 2(4), GROUP 3(5), GROUP 4(6)"
			::= { neUserAccountEntry 4 }

		neGroupAccessTable OBJECT-TYPE
			SYNTAX SEQUENCE OF NeGroupAccessEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"Table containing information about each Users."
			::= { user_accounts 30 }

		neGroupAccessEntry OBJECT-TYPE
			SYNTAX NeGroupAccessEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"List of information about each Users."
			INDEX { neUserAccountIndex }
			::= { neGroupAccessTable 1 }

		NeGroupAccessEntry ::=
			SEQUENCE { 
				neGroupAccessIndex
					INTEGER,
				neGroup1AccessPage
					DisplayString,
				neGroup1AccessLevel
