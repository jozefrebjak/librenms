			SYNTAX INTEGER (0..1)
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Clear Logbook: false(0), true(1)"
			::= { system 52 }

		neConfigDateTimeSyncMode OBJECT-TYPE
			SYNTAX INTEGER (0..5)
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Date-Time Sync Mode: OFF(0), NTP SERVER(1), TS1(2), TS2(3), TS3(4), TS4(5)"
			::= { date_time 1 }

 		neConfigDate OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..16))
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Date: dd.mm.yy"
			::= { date_time 2 }

 		neConfigTime OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..16))
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Time: hh:mm:ss"
			::= { date_time 3 }

 		neConfigTimeOffset OBJECT-TYPE
			SYNTAX INTEGER (-24..24)
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"Time Offset: +12.0h(24),  +11.5h(23), +11.0h(22)  +10.5h(21),   +10.0h(20),   +9.5h(19),
				+9.0h(18),     +8.5h(17),  +8.0h(16)   +7.5h(15),   +7.0h(14),    +6.5h(13)    +6.0h(12),
				+5.5h(11),     +5.0h(10)   +4.5h(9),   +4.0h(8),    +3.5h(7),     +3.0h(6),    +2.5h(5),
				+2.0h(4),      +1.5h(3),   +1.0h(2)    +0.5h(1),    +0.0h(0),     -0.5h(-1),   -1.0h(-2),
				-1.5h(-3),     -2.0h(-4)   -2.5h(-5),  -3.0h(-6),   -3.5h(-7),    -4.0h(-8),   -4.5h(-9),
				-5.0h(-10),    -5.5h(-11), -6.0h(-12)  -6.5h(-13),  -7.0h(-14),   -7.5h(-15),  -8.0h(-16),
				-8.5h(-17),    -9.0h(-18)  -9.5h(-19), -10.0h(-20), -10.5h(-21), -11.0h(-22), -11.5h(-23),
				-12.0h(-24)"
			::= { date_time 4 }

		neConfigNtpServer1 OBJECT-TYPE
			SYNTAX IpAddress
			ACCESS read-write
			STATUS mandatory
			DESCRIPTION
				"NTP Server 1:"
			::= { date_time 5 }

