digital, DisplayString, TruthValue,  NESlotValue,                              
PerceivedSeverityValue, InitPhaseValue                                         
FROM BLANKOM-ROOT-MIB;                                                         
                                                                               
--                                                                             
-- Node definitions                                                            
--                                                                             
drd700 OBJECT IDENTIFIER ::= { digital 45 }                                    
ne OBJECT IDENTIFIER ::= { drd700 1 }                                          
neCommon OBJECT IDENTIFIER ::= { ne 1 }
neConfig OBJECT IDENTIFIER ::= { ne 3 }
input OBJECT IDENTIFIER ::= { neConfig 10 }
ts1 OBJECT IDENTIFIER ::= { input 20 }
ts2 OBJECT IDENTIFIER ::= { input 25 }
ts3 OBJECT IDENTIFIER ::= { input 30 }
ts4 OBJECT IDENTIFIER ::= { input 35 }
dvb-s-s2 OBJECT IDENTIFIER ::= { input 200 }
dvb-t-t2 OBJECT IDENTIFIER ::= { input 210 }
dvb-c-c2 OBJECT IDENTIFIER ::= { input 220 }
input-ip OBJECT IDENTIFIER ::= { input 230 }
common-interface OBJECT IDENTIFIER ::= { neConfig 20 }
slot1 OBJECT IDENTIFIER ::= { common_interface 20 }
slot2 OBJECT IDENTIFIER ::= { common_interface 30 }
slot3 OBJECT IDENTIFIER ::= { common_interface 40 }
slot4 OBJECT IDENTIFIER ::= { common_interface 50 }
mux OBJECT IDENTIFIER ::= { neConfig 30 }
mux1 OBJECT IDENTIFIER ::= { mux 20 }
mux2 OBJECT IDENTIFIER ::= { mux 30 }
mux3 OBJECT IDENTIFIER ::= { mux 40 }
mux4 OBJECT IDENTIFIER ::= { mux 50 }
output OBJECT IDENTIFIER ::= { neConfig 40 }
output-ip OBJECT IDENTIFIER ::= { output 30 }
mpts OBJECT IDENTIFIER ::= { output_ip 10 }
spts OBJECT IDENTIFIER ::= { output_ip 50 }
output_asi OBJECT IDENTIFIER ::= { output 40 }
lan OBJECT IDENTIFIER ::= { neConfig 50 }
snmpv2 OBJECT IDENTIFIER ::= { lan 50 }
telnet OBJECT IDENTIFIER ::= { lan 60 }
redundancy OBJECT IDENTIFIER ::= { lan 70 }
system OBJECT IDENTIFIER ::= { neConfig 60 }
date-time OBJECT IDENTIFIER ::= { system 100 }
user_accounts OBJECT IDENTIFIER ::= { neConfig 70 }

--
-- Type definitions
--
ConfigNETypeValue ::= INTEGER
{
  headend1(1),
  ringhub1(20)
}

		neType OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..32))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Displays the type of the NE. Some agents have configurable types
				(neConfigNEtype), some agents have one fixed type."
			::= { neCommon 1 }

		neStates OBJECT IDENTIFIER ::= { ne 2 }

		neStatesBootloaderVersion OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..32))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Bootloader Version"
			::= { neStates 1 }

		neStatesApplicationVersion OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..32))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Application Version"
			::= { neStates 2 }

		neStatesFPGAVersion OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..32))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"FPGA Version"
			::= { neStates 4 }

		neStatesFPGACpuVersion OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..32))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"FPGA Cpu Version"
			::= { neStates 5 }

		neStatesDeviceStatus OBJECT-TYPE
			SYNTAX INTEGER
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"DeviceStatus ( 
						Bit#1  = DMS STATUS FAILURE
						Bit#2  = INPUT SIGNAL STATUS FAILURE
						Bit#3  = BER STATUS FAILURE
						Bit#4  = AFC STATUS FAILURE
						Bit#5  = FAN STATUS FAILURE
						Bit#6  = POWER STATUS_FAILURE
						Bit#7  = FPGA STATUS FAILURE
						Bit#8  = FE_BOARD_1 STATUS FAILURE
						Bit#9  = FE_BOARD_2 STATUS FAILURE
						Bit#10 = I2C STATUS FAILURE
						Bit#11 = DISPLAY STATUS FAILURE
						Bit#12 = unused
						Bit#13 = unused
						Bit#14 = unused
						Bit#15 = unused
						Bit#16 = unused
						Bit#17 = unused
						Bit#18 = unused
						Bit#19 = unused
						Bit#20 = unused
						Bit#21 = unused
						Bit#22 = unused
						Bit#23 = unused
						Bit#24 = unused
						Bit#25 = unused
						Bit#26 = unused
						Bit#27 = unused
						Bit#28 = unused
						Bit#29 = unused
						Bit#30 = unused
						Bit#31 = unused
						Bit#32 = unused
						)."
			::= { neStates 10  }

		neStatesMacAddress1 OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..16))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Mac Address 1"
			::= { neStates 11 }

		neStatesMacAddress2 OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..16))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Mac Address 2"
			::= { neStates 12 }

		neStatesDeviceSerialNumber OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..16))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Device Serial Number"
			::= { neStates 13 }

		neStatesDeviceType OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..16))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Device Type"
			::= { neStates 14 }

		neStatesDeviceLedInput OBJECT-TYPE
			SYNTAX INTEGER (1..4)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"LED Input ( 1 = green, 2 = red, 3 = orange, 4 = off )."
			::= { neStates 15  }

		neStatesDeviceLedStatus OBJECT-TYPE
			SYNTAX INTEGER (1..4)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"LED Status ( 1 = green, 2 = red, 3 = orange, 4 = off )."
			::= { neStates 16  }

		neStatesDeviceLedFault OBJECT-TYPE
			SYNTAX INTEGER (1..4)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"LED Fault ( 1 = green, 2 = red, 3 = orange, 4 = off )."
			::= { neStates 17  }

		neLicenseTable OBJECT-TYPE
			SYNTAX SEQUENCE OF NeLicenseEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"License."
			::= { neStates 40 }

		neLicenseEntry OBJECT-TYPE
			SYNTAX NeLicenseEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"Logbook."
			INDEX { neLicenseIndex }
			::= { neLicenseTable 1 }

		NeLicenseEntry ::=
			SEQUENCE { 
				neLicenseIndex
					INTEGER,
				neLicenseDescription
					DisplayString,
				neLicenseStatus
					INTEGER
			 }

		neLicenseIndex OBJECT-TYPE
			SYNTAX INTEGER (1..256)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Index Position."
			::= { neLicenseEntry 1 }

		neLicenseDescription OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..64))
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"License: Description"
			::= { neLicenseEntry 2 }

		neLicenseStatus OBJECT-TYPE
			SYNTAX INTEGER (1..2)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"License: disabled(1), enabled(2)"
			::= { neLicenseEntry 3 }

		neMessageTable OBJECT-TYPE
			SYNTAX SEQUENCE OF NeMessageEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"License."
			::= { neStates 45 }

		neMessageEntry OBJECT-TYPE
			SYNTAX NeMessageEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"Message."
			INDEX { neMessageIndex }
			::= { neMessageTable 1 }

		NeMessageEntry ::=
			SEQUENCE { 
				neMessageIndex
					INTEGER,
				neMessageSeverity
					INTEGER
			 }

		neMessageIndex OBJECT-TYPE
			SYNTAX INTEGER (1..256)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Index Position:
				
				 1  => INPUT
				 2  => STATUS
				 3  => FAULT
				"
			::= { neMessageEntry 1 }

		neMessageSeverity OBJECT-TYPE
			SYNTAX INTEGER (1..2)
			ACCESS read-only
			STATUS mandatory
			DESCRIPTION
				"Message Severity: Cleared(1), Alarm(2)"
			::= { neMessageEntry 2 }

		neLogbookTable OBJECT-TYPE
			SYNTAX SEQUENCE OF NeLogbookEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"Logbook."
			::= { neStates 50 }

		neLogbookEntry OBJECT-TYPE
			SYNTAX NeLogbookEntry
			ACCESS not-accessible
			STATUS mandatory
			DESCRIPTION
				"Logbook."
			INDEX { neLogbookIndex }
			::= { neLogbookTable 1 }

		NeLogbookEntry ::=
			SEQUENCE { 
				neLogbookIndex
					INTEGER,
				neLogbookStatus
